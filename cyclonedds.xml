<?xml version="1.0" encoding="UTF-8"?>
<CycloneDDS>
  <Domain id="any">
    <General>
      <Interfaces>
        <NetworkInterface name="eth0"/>
      </Interfaces>
      <AllowMulticast>true</AllowMulticast>
    </General>
    <Tracing>
      <Verbosity>fine</Verbosity>
      <OutputFile>/tmp/cyclonedds.log</OutputFile>
    </Tracing>
  </Domain>
</CycloneDDS>
